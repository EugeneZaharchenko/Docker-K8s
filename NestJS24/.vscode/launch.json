{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "launch",
      "name": "Debug NestJS (ts-node)",
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "${workspaceFolder}/nest-js24/src/main.ts"
      ],
      "cwd": "${workspaceFolder}/nest-js24",
      "protocol": "inspector",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/nest-js24/tsconfig.json"
      },
      "sourceMaps": true,
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "internalConsoleOptions": "neverOpen"
    }
  ]
}